{
  "name": "windranger-treasury",
  "version": "0.0.1",
  "description": "The WindRanger-io Treasury specifications and implementation",
  "private": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windranger-io/windranger-treasury"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "hardhat compile",
    "clean": "hardhat clean",
    "format": "npm run format-mjs && npm run format-sol && npm run format-ts",
    "format-mjs": "prettier '**/*.mjs' --write",
    "format-sol": "prettier '**/*.sol' --write",
    "format-ts": "prettier '**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "plant": "node ./scripts/plantuml.mjs ./docs/specs",
    "prepare": "husky install",
    "test": "mocha --timeout 10000 --exit --recursive --require ts-node/register 'test/**/*.test.ts'"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/bunyan": "^1.8.7",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.6.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.2",
    "node-plantuml": "github:shahankhatch/node-plantuml",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "@openzeppelin/contracts": "^4.3.2"
  }
}
